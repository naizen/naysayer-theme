    "statusBarItem.remoteBackground": "#d3b58d",
    "statusBarItem.remoteForeground": "#ffffff",
    "titleBar.activeBackground": "#031313",
    "pickerGroup.border": "#FFFFFF1a",
    "debugToolBar.background": "#052121",
    "selection.background": "#d3b58d"
  }
}
